  "license": "ISC",
  "dependencies": {
    "axios": "^1.1.3",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "nodemon": "^2.0.20"
  },
  "devDependencies": {
    "@prisma/client": "^4.5.0",
    "@types/cors": "^2.8.12",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "jest": "^29.2.2",
    "prisma": "^4.5.0",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
